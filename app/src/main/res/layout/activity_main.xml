<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android = "http://schemas.android.com/apk/res/android"
                                             xmlns:tools = "http://schemas.android.com/tools"
                                             xmlns:app = "http://schemas.android.com/apk/res-auto"
                                             android:layout_width = "match_parent"
                                             android:layout_height = "match_parent"
                                             tools:context = "com.hisuchina.tinkertest.MainActivity"
android:background="@color/colorAccent">


	<Button
			android:id="@+id/load_patch_btn"
			android:layout_width = "match_parent"
	        android:layout_height = "wrap_content"
	        android:text="load patch"
	        android:onClick="loadPatch" />
	<Button android:layout_width = "match_parent"
	        android:layout_height = "wrap_content"
	        android:text="测试按钮"
	        android:onClick="testBtn"
	        app:layout_constraintTop_toBottomOf="@+id/load_patch_btn"
	        tools:ignore = "OnClick" />
</android.support.constraint.ConstraintLayout>
